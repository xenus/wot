<?xml version="1.0" encoding="utf-8"?>
<!-- *** Other languages are supported for texts *** -->
<!-- *** To use them here you MUST switch encoding into 'UTF-8 without BOM'! *** -->
<!-- *** Official topic: http://www.koreanrandom.com/forum/topic/16559-beta-улучшенная-система-прицеливания-advanced-aiming-system/ *** -->
<root>
	<!-- Global parameter. Enables mod loading. -->
	<!-- Глобальный переключатель, загружать мод или нет. -->
	<modEnabled>True</modEnabled>
	<!-- Ignore client version. For advanced users only! -->
	<!-- Игнорировать версию клиента. Для опытных пользователей! -->
	<ignoreClientVersion>False</ignoreClientVersion>
	<!-- Time between mod loading and hook injecting. For advanced users only! -->
	<!-- Время между загрузкой мода и установкой хуков. Для опытных пользователей! -->
	<hookSetTimeout>3.0</hookSetTimeout>
	<!-- Text message in hangar on mod loaded. -->
	<!-- Сообщение в ангаре при загрузке мода. -->
	<modLoadedMessage>"Улучшенная система прицеливания" загружена.</modLoadedMessage>
	<!-- Text message in hangar on mod loaded - update required. -->
	<!-- Сообщение в ангаре при загрузке мода - необходимо обновление. -->
	<modUpdateMessage>"Улучшенная система прицеливания" нуждается в обновлении.</modUpdateMessage>
	<!-- Common parameters configuration section (for all aim modes). -->
	<!-- Блок конфигурации всех режимов прицеливания (общие параметры). -->
	<commonAS>
		<!-- Enable X-Ray target lock for standard auto-aim. -->
		<!-- Разрешить рентген-захват цели в автоприцел. -->
		<enableXRayLockForAutoAim>True</enableXRayLockForAutoAim>
		<!-- Enable X-Ray target lock for radial menu. -->
		<!-- Разрешить рентген-захват цели для радиального меню. -->
		<enableXRayLockForRadialMenu>True</enableXRayLockForRadialMenu>
		<!-- Enable BoundingBox target lock for standard auto-aim. -->
		<!-- Разрешить BoundingBox-захват цели в автоприцел. -->
		<enableBBoxLockForAutoAim>False</enableBBoxLockForAutoAim>
		<!-- BoundingBox size multiplier. -->
		<!-- Множитель размера BoundingBox. -->
		<vehicleBBoxMultiplier>2.0</vehicleBBoxMultiplier>
		<!-- Speed multiplier (used for target speed display). -->
		<!-- Might be used for unit conversion. 3.6 for km/h, 1.94384449 for miles/h. -->
		<!-- Множитель скорости цели при отображении на экране. -->
		<!-- Используется для перевода единиц измерения. Для км/ч 3.6, для миль/час 1.94384449. -->
		<speedDisplayMultiplier>1.0</speedDisplayMultiplier>
		<!-- Allies and corpse shot blocker configuration section. -->
		<!-- Блок конфигурации блокировки стрельбы по союзникам и трупам. -->
		<safeShot>
			<!-- Enable shot blocking. -->
			<!-- Разрешить блокировку стрельбы. -->
			<enabled>True</enabled>
			<!-- Hot-key for switch off. -->
			<!-- "Горячая клавиша" отключения блокировки. -->
			<switchKey>KEY_LALT</switchKey>
			<!-- Hot-key mode (True - switch, False - button). -->
			<!-- In button mode block off then pressed (only here). -->
			<!-- Режим работы "горячей клавиши" (True - переключатель, False - кнопка). -->
			<!-- В режиме кнопки блокировка отключается при нажатии (только здесь). -->
			<keySwitchMode>False</keySwitchMode>
			<!-- Enables block by default. -->
			<!-- Активировать блокировку по умолчанию. -->
			<activated>True</activated>
			<!-- Block activation text message. -->
			<!-- Сообщение при активации блокировки. -->
			<activateMessage>SafeShot включен.</activateMessage>
			<!-- Block deactivation text message. -->
			<!-- Сообщение при деактивации блокировки. -->
			<deactivateMessage>SafeShot отключен.</deactivateMessage>
			<!-- In-sight text message then shot is blocked.  -->
			<!-- Сообщение над прицелом при блокировке выстрела. -->
			<shotBlockMessage>[{{shotBlockReason}}] Выстрел заблокирован.</shotBlockMessage>
			<!-- Block reason text configuration section. -->
			<!-- Блок конфигурации текста причины блокировки выстрела. -->
			<shotBlockReasons>
				<!-- Friendly fire. -->
				<!-- Огонь по союзникам. -->
				<teamShot>teamShot</teamShot>
				<!-- Corpse shooting. -->
				<!-- Стрельба в труп. -->
				<deadShot>deadShot</deadShot>
				<!-- Useless fire. -->
				<!-- Стрельба заведомо мимо цели. -->
				<wasteShot>wasteShot</wasteShot>
			</shotBlockReasons>
			<!-- Ally shot block configuration section. -->
			<!-- Блок конфигурации блокировки стрельбы по союзникам. -->
			<teamShot>
				<!-- Block friendly fire. -->
				<!-- Блокировать стрельбу. -->
				<blockShot>True</blockShot>
				<!-- Block traitor fire like friendly.  -->
				<!-- Блокировать стрельбу по "голубцам" так же, как и по нормальным союзникам. -->
				<blockBlueShot>False</blockBlueShot>
				<!-- Consider gun direction. -->
				<!-- Учитывать направление орудия. -->
				<considerGunDirection>True</considerGunDirection>
				<!-- Send message in chat then shot is blocked. -->
				<!-- Отправлять сообщение в чат при блокировке выстрела. -->
				<sendChatMessage>True</sendChatMessage>
				<!-- Equal message send interval. Spam protection. -->
				<!-- Интервал отправки одинаковых сообщений. -->
				<chatMessageInterval>5.0</chatMessageInterval>
				<!-- Chat message template. Allowed macros {{name}} and {{vehicle}}. -->
				<!-- Текст сообщения. Разрешены макросы {{name}} и {{vehicle}}. -->
				<chatMessage>{{name}} ({{vehicle}}), не мешай стрелять!</chatMessage>
			</teamShot>
			<!-- Corpse shot block configuration section. -->
			<!-- Блок конфигурации блокировки стрельбы по трупам. -->
			<deadShot>
				<!-- Block corpse fire. -->
				<!-- Блокировать стрельбу. -->
				<blockShot>True</blockShot>
				<!-- Corpse shooting block time (after destruction). -->
				<!-- Время блокировки огня по цели после ее уничтожения. -->
				<blockTimeout>2.0</blockTimeout>
			</deadShot>
			<!-- Useless shot block configuration section. -->
			<!-- Блок конфигурации блокировки бесполезной стрельбы. -->
			<wasteShot>
				<!-- Block useless fire. -->
				<!-- Блокировать стрельбу. -->
				<blockShot>False</blockShot>
			</wasteShot>
		</safeShot>
		<!-- Expert perk improvement configuration section. -->
		<!-- Блок конфигурации улучшения перка "Эксперт". -->
		<advancedExpertPerk>
			<!-- Enable Expert perk improvement. -->
			<!-- Разрешить улучшение перка "Эксперт". -->
			<enabled>False</enabled>
			<!-- Allow received data caching. -->
			<!-- Разрешить кэширование получаемых данных. -->
			<cacheExtrasInfo>True</cacheExtrasInfo>
			<!-- Enable request queue. -->
			<!-- Разрешить очередь запросов. -->
			<enableQueue>False</enableQueue>
			<!-- Request waiting time. For advanced users only! -->
			<!-- Время ожидания ответа на запрос. Для опытных пользователей! -->
			<replyInterval>5.0</replyInterval>
			<!-- Next request delay. For advanced users only! -->
			<!-- Время ожидания формирования нового запроса. Для опытных пользователей! -->
			<requestInterval>5.0</requestInterval>
		</advancedExpertPerk>
	</commonAS>
	<!-- Arcade mode configuration section. -->
	<!-- Блок конфигурации аркадного режима. -->
	<arcadeAS>
		<!-- Sniper mode switch hot-key (for SPG only). -->
		<!-- "Горячая клавиша" переключения в снайперский режим (только для артиллерии). -->
		<sniperModeKeySPG>KEY_E</sniperModeKeySPG>
		<!-- Aiming information GUI configuration section. -->
		<!-- Информация о сведении на экране. -->
		<aimingInfo>
			<!-- Enable aiming info GUI. -->
			<!-- Показывать информацию на экране. -->
			<enabled>True</enabled>
			<!-- Hot-key for switch off. -->
			<!-- "Горячая клавиша" отключения. -->
			<switchKey>KEY_LCONTROL+KEY_A</switchKey>
			<!-- Hot-key mode (True - switch, False - button). -->
			<!-- Режим работы "горячей клавиши" (True - переключатель, False - кнопка). -->
			<keySwitchMode>True</keySwitchMode>
			<!-- Enables by default. -->
			<!-- Активировать по умолчанию. -->
			<activated>False</activated>
			<!-- Info panel texture. -->
			<!-- Текстура панели информации. -->
			<bgTextureName>gui/maps/ingame/aim/aimingInfoBG.dds</bgTextureName>
			<!-- Additional colour of info panel. -->
			<!-- Дополнительный цвет панели информации. -->
			<bgAddColour>0 0 0 127</bgAddColour>
			<!-- Info panel size. -->
			<!-- Размер панели информации. -->
			<windowSize>205 85</windowSize>
			<!-- Position of info panel on screen. -->
			<!-- Положение панели информации на экране. -->
			<windowPosition>0.4 -0.1 1.0</windowPosition>
			<!-- Info panel text colour. -->
			<!-- Цвет текста на панели информации. -->
			<labelColour>100 180 240 255</labelColour>
			<!-- Info panel text template. -->
			<!-- Шаблон текста на панели информации. -->
			<labelTemplate>Осталось: {{remainingAimingTime}}с;\nДистанция: {{aimingDistance}}м;\nРазброс: {{deviation}}м;\nВремя полета: {{flyTime}}с;</labelTemplate>
		</aimingInfo>
	</arcadeAS>
	<!-- Sniper mode configuration section. -->
	<!-- Блок конфигурации снайперского режима. -->
	<sniperAS>
		<!-- Target lock configuration section. -->
		<!-- Захват цели. -->
		<targetLock>
			<!-- Enable X-Ray target lock. -->
			<!-- Разрешить рентген-захват цели. -->
			<enableXRayLock>True</enableXRayLock>
			<!-- Enable manual target lock. -->
			<!-- Разрешить ручной захват цели. -->
			<enableManualTargetLock>False</enableManualTargetLock>
			<!-- Manual target lock hot-key. -->
			<!-- "Горячая клавиша" ручного захвата цели. -->
			<manualTargetLockKey>KEY_T</manualTargetLockKey>
			<!-- Enable auto-lock on hover. -->
			<!-- Разрешить автоматический захват при наведении. -->
			<enableAutoLock>True</enableAutoLock>
			<!-- Enable auto-lock only for hostiles. -->
			<!-- Автоматический захват только противников. -->
			<autoLockOnlyEnemies>True</autoLockOnlyEnemies>
			<!-- Auto-lock reset timeout (if target is not in sight for this time). -->
			<!-- Если цель не попадала в прицел в течение этого времени - сброс цели. -->
			<autoLockTargetResetTimeout>3.0</autoLockTargetResetTimeout>
			<!-- Info about locked target on screen. -->
			<!-- Информация о захваченной цели на экране. -->
			<targetInfoGUI>
				<!-- Show info about locked target. -->
				<!-- Показывать информацию о захваченной цели. -->
				<enabled>True</enabled>
				<!-- Template of info. Allowed {{targetShortName}} and {{targetSpeed}} macros. -->
				<!-- Шаблон выводимой информации. Доступны макросы {{targetShortName}} и {{targetSpeed}}. -->
				<template>Цель: "{{targetShortName}}"; Скорость: {{targetSpeed}}м/с.</template>
				<!-- Text position on screen. -->
				<!-- Позиция на экране. -->
				<position>0 0.30 1.0</position>
				<!-- Text colour. -->
				<!-- Цвет текста. -->
				<colour>255 127 0 255</colour>
			</targetInfoGUI>
		</targetLock>
		<!-- Distance lock configuration section. -->
		<!-- Захват расстояния до цели. -->
		<distanceLock>
			<!-- Enable manual distance lock. -->
			<!-- Разрешить ручной захват расстояния до цели. -->
			<enableManualDistanceLock>True</enableManualDistanceLock>
			<!-- Manual distance lock hot-key. -->
			<!-- "Горячая клавиша" захвата расстояния. -->
			<manualDistanceLockKey>KEY_LALT</manualDistanceLockKey>
			<!-- Use TargetLock module. -->
			<!-- Использовать модуль TargetLock. -->
			<useTargetLock>True</useTargetLock>
			<!-- Info about locked distance on screen. -->
			<!-- Информация о захваченной дистанции на экране. -->
			<distanceInfoGUI>
				<!-- Show info about locked distance. -->
				<!-- Показывать информацию о захваченном расстоянии. -->
				<enabled>True</enabled>
				<!-- Template of info. Allowed {{distance}} macros. -->
				<!-- Шаблон выводимой информации. Доступен макрос {{distance}}. -->
				<template>Дальномер зафиксирован: {{distance}}м.</template>
				<!-- Text position on screen. -->
				<!-- Позиция на экране. -->
				<position>0 0.25 1.0</position>
				<!-- Text colour when target may be hit. -->
				<!-- Цвет текста, когда цель в зоне поражения. -->
				<affectedColour>0 255 0 255</affectedColour>
				<!-- Text colour when target is too far. -->
				<!-- Цвет текста, когда цель вне зоны поражения. -->
				<unaffectedColour>255 0 0 255</unaffectedColour>
			</distanceInfoGUI>
		</distanceLock>
		<!-- Deflection marker (calculates point to shoot for moving targets). -->
		<!-- Маркер упреждения. -->
		<deflectionMarker>
			<!-- Enable deflection marker (may decrease FPS when activated). -->
			<!-- Разрешить маркер упреждения (может снижать FPS, когда активирован). -->
			<enabled>False</enabled>
			<!-- Deflection marker activation hot-key. -->
			<!-- "Горячая клавиша" активации маркера упреждения. -->
			<switchKey>KEY_LCONTROL+KEY_D</switchKey>
			<!-- Hot-key mode (True - switch, False - button). -->
			<!-- Режим работы "горячей клавиши" (True - переключатель, False - кнопка). -->
			<keySwitchMode>True</keySwitchMode>
			<!-- Activates deflection marker by default. -->
			<!-- Активировать маркер упреждения по умолчанию. -->
			<activated>False</activated>
			<!-- Deflection marker activation text message. -->
			<!-- Сообщение при активации маркера упреждения. -->
			<activateMessage>Вычисление упреждения включено.</activateMessage>
			<!-- Deflection marker deactivation text message. -->
			<!-- Сообщение при деактивации маркера упреждения. -->
			<deactivateMessage>Вычисление упреждения отключено.</deactivateMessage>
			<!-- Deflection marker texture. -->
			<!-- Текстура маркера упреждения. -->
			<textureName>gui/maps/ingame/aim/deflectionMarker.dds</textureName>
			<!-- Deflection marker size. -->
			<!-- Размер маркера упреждения. -->
			<textureSize>15 15</textureSize>
		</deflectionMarker>
		<aimingInfo>
			<enabled>True</enabled>
			<switchKey>KEY_LCONTROL+KEY_A</switchKey>
			<keySwitchMode>True</keySwitchMode>
			<activated>False</activated>
			<bgTextureName>gui/maps/ingame/aim/aimingInfoBG.dds</bgTextureName>
			<bgAddColour>0 0 0 127</bgAddColour>
			<windowSize>205 85</windowSize>
			<windowPosition>0.4 -0.25 1.0</windowPosition>
			<labelColour>100 240 180 255</labelColour>
			<labelTemplate>Осталось: {{remainingAimingTime}}с;\nДистанция: {{aimingDistance}}м;\nРазброс: {{deviation}}м;\nВремя полета: {{flyTime}}с;</labelTemplate>
		</aimingInfo>
	</sniperAS>
	<!-- Strategic mode configuration section. -->
	<!-- Блок конфигурации артиллерийского режима. -->
	<strategicAS>
		<!-- Target lock configuration section. -->
		<!-- Захват цели. -->
		<targetLock>
			<enableXRayLock>True</enableXRayLock>
			<enableManualTargetLock>False</enableManualTargetLock>
			<manualTargetLockKey>KEY_T</manualTargetLockKey>
			<enableAutoLock>True</enableAutoLock>
			<autoLockOnlyEnemies>True</autoLockOnlyEnemies>
			<autoLockTargetResetTimeout>10.0</autoLockTargetResetTimeout>
			<targetInfoGUI>
				<enabled>True</enabled>
				<template>Цель: "{{targetShortName}}"; Скорость: {{targetSpeed}}м/с.</template>
				<position>0 0.30 1.0</position>
				<colour>255 127 0 255</colour>
			</targetInfoGUI>
		</targetLock>
		<!-- Absolute height lock configuration section. -->
		<!-- Захват абсолютной высоты. -->
		<heightLock>
			<!-- Enable manual absolute height lock. -->
			<!-- Разрешить ручной захват абсолютной высоты. -->
			<enableManualHeightLock>True</enableManualHeightLock>
			<!-- Manual absolute height lock hot-key. -->
			<!-- "Горячая клавиша" ручного захвата абсолютной высоты. -->
			<manualHeightLockKey>KEY_LALT</manualHeightLockKey>
			<!-- Tank height locking on target lock. -->
			<!-- Захват относительной высоты (высоты танка) при захвате цели. -->
			<relativeHeightLock>
				<!-- Enable tank height lock. -->
				<!-- Разрешить захват высоты танка. -->
				<enabled>True</enabled>
				<!-- Ignore vehicles. You'll aim at the ground under vehicles. -->
				<!-- Игнорировать технику. Прицеливание в землю под танком. -->
				<ignoreVehicles>True</ignoreVehicles>
				<!-- Tank height lock activation hot-key. -->
				<!-- "Горячая клавиша" включения захвата высоты танка. -->
				<switchKey>KEY_LCONTROL+KEY_H</switchKey>
				<!-- Hot-key mode (True - switch, False - button). -->
				<!-- Режим работы "горячей клавиши" (True - переключатель, False - кнопка). -->
				<keySwitchMode>True</keySwitchMode>
				<!-- Activates tank height lock by default. -->
				<!-- Активировать захват высоты танка по умолчанию. -->
				<activated>False</activated>
				<!-- Tank height multiplier. Optimal is 0.3..0.5. -->
				<!-- Множитель высоты танка. Оптимально 0.3..0.5. -->
				<targetHeightMultiplier>0.5</targetHeightMultiplier>
				<!-- Tank height lock activation text message. -->
				<!-- Сообщение при активации режима захвата высоты танка. -->
				<activateMessage>Учёт высоты цели включен.</activateMessage>
				<!-- Tank height lock deactivation text message. -->
				<!-- Сообщение при деактивации режима захвата высоты танка. -->
				<deactivateMessage>Учёт высоты цели отключен.</deactivateMessage>
			</relativeHeightLock>
			<!-- Info about locked height on screen. -->
			<!-- Информация о захваченной высоте на экране. -->
			<heightInfoGUI>
				<!-- Show info about locked height. -->
				<!-- Показывать информацию о захваченной высоте. -->
				<enabled>True</enabled>
				<!-- Template of info. Allowed {{height}} and {{relativeHeight}} macros. -->
				<!-- Шаблон выводимой информации. Доступны макросы {{height}} и {{relativeHeight}}. -->
				<template>Высотомер зафиксирован: {{height}}м. Отн. высота: {{relativeHeight}}</template>
				<!-- Text position on screen. -->
				<!-- Позиция на экране. -->
				<position>0 0.25 1.0</position>
				<!-- Text colour. -->
				<!-- Цвет текста. -->
				<colour>0 255 0 255</colour>
			</heightInfoGUI>
		</heightLock>
		<!-- Artillery-sniper mode. -->
		<!-- Артоснайперский режим. -->
		<strategicSniperMode>
			<!-- Enable artillery-sniper mode. -->
			<!-- Разрешить артоснайперский режим. -->
			<enabled>True</enabled>
			<!-- Artillery-sniper mode activation hot-key. -->
			<!-- "Горячая клавиша" активации артоснайперского режима. -->
			<switchKey>KEY_LCONTROL+KEY_S</switchKey>
			<!-- Hot-key mode (True - switch, False - button). -->
			<!-- Режим работы "горячей клавиши" (True - переключатель, False - кнопка). -->
			<keySwitchMode>True</keySwitchMode>
			<!-- Base plane level. For advanced users only! -->
			<!-- Уровень базовой плоскости. Для опытных пользователей! -->
			<zeroPlaneLevel>-150.0</zeroPlaneLevel>
			<!-- Camera base pitch. -->
			<!-- Базовый угол наклона камеры. -->
			<cameraBasePitch>0.0</cameraBasePitch>
			<!-- Camera base pitch up hot-key. -->
			<!-- "Горячая клавиша" увеличения базового угла наклона камеры. -->
			<cameraBasePitchUpKey>KEY_LCONTROL+KEY_R</cameraBasePitchUpKey>
			<!-- Camera base pitch down hot-key. -->
			<!-- "Горячая клавиша" уменьшения базового угла наклона камеры. -->
			<cameraBasePitchDnKey>KEY_LCONTROL+KEY_F</cameraBasePitchDnKey>
			<!-- Enable message on camera base pitch change.  -->
			<!-- Разрешить сообщение при изменении базового угла камеры. -->
			<cameraBasePitchChangeShowMessage>True</cameraBasePitchChangeShowMessage>
			<!-- Camera base pitch change message. Allowed {{basePitch}} and {{basePitchDelta}} macros. -->
			<!-- Сообщение при изменении базового угла камеры. Разрешены макросы {{basePitch}} и {{basePitchDelta}}. -->
			<cameraBasePitchChangeMessage>Базовый угол камеры >>> {{basePitch}} ({{basePitchDelta}}).</cameraBasePitchChangeMessage>
			<!-- Enable max distance correction (may decrease FPS when activated). -->
			<!-- Коррекцию максимального расстояния (может снижать FPS, когда активировано). -->
			<maxDistanceCorrection>False</maxDistanceCorrection>
			<!-- Hold camera in some range to player's vehicle. -->
			<!-- Удерживать камеру в пределах некоторого расстояния до техники игрока. -->
			<rangeLockEnabled>True</rangeLockEnabled>
			<!-- Range update hot-key. Calculated by aim point. -->
			<!-- "Горячая клавиша" обновления расстояния по точке прицеливания. -->
			<rangeLockResetKey>KEY_SPACE</rangeLockResetKey>
		</strategicSniperMode>
		<aimingInfo>
			<enabled>True</enabled>
			<switchKey>KEY_LCONTROL+KEY_A</switchKey>
			<keySwitchMode>True</keySwitchMode>
			<activated>False</activated>
			<bgTextureName>gui/maps/ingame/aim/aimingInfoBG.dds</bgTextureName>
			<bgAddColour>0 0 0 127</bgAddColour>
